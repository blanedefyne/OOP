<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.nsu.zelenin.snake.WinController">
   <Label alignment="CENTER_RIGHT" layoutX="100.0" layoutY="26.0" prefHeight="71.0" prefWidth="199.0"
          text="YOU'VE WON!">
      <font>
         <Font size="30.0"/>
      </font>
   </Label>
   <Button layoutX="32.0" layoutY="123.0" mnemonicParsing="false" onAction="#playAgain" prefHeight="44.0"
           prefWidth="114.0" text="Play again!"/>
   <Button layoutX="250.0" layoutY="123.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="44.0"
           prefWidth="114.0" text="Quit game"/>
</AnchorPane>
